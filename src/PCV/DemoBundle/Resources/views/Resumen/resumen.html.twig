{% extends 'PCVDemoBundle:Plantillas:page.html.twig' %} 
{% block title datosU.name %}

{% block navbar %}
	{% embed 'PCVDemoBundle:Plantillas:pagenav.html.twig' %}
		{% block navbar %}
			{{parent()}}
		{% endblock %}
	{% endembed %}
{% endblock %}

{% block contenido %}
<div class="jumbotron wrapper" style="background-color:#f5f5f5">
	<div class="container wrapper">
	{% embed 'PCVDemoBundle:Show:portada.html.twig' %}
		{% block portada_private %}
			{{parent()}}
		{% endblock %}
		{% endembed %}
	</div>
</div>

<div class="jumbotron" style="background-color:#fff">
	<div class="container">
		<div class="page-header" style="border-bottom-color:#ddd">
          <h3 class="text-muted"><i class="fa fa-align-center muted"></i>&nbsp;Resumen</h3>
        </div>

		{% embed 'PCVDemoBundle:Show:bio.html.twig' %}
			{% block bio_private %}
				{{parent()}}
			{% endblock %}
			{% block modal_bio %}{{parent()}}{% endblock %}
		{% endembed %}
	</div>
</div>


<div class="jumbotron" style="background-color:#f5f5f5">
	<div class="container">
		<div class="page-header" style="border-bottom-color:#ddd">
          <h3 class="text-muted"><i class="fa fa-user muted"></i>&nbsp;Datos Personales</h3>
        </div>

		{% embed 'PCVDemoBundle:Show:datos.html.twig' %}
			{% block data_private %}
				{{parent()}}
			{% endblock %}
			{% block modal_data %}{{parent()}}{% endblock %}
		{% endembed %}
	</div>
</div>

<div class="jumbotron" style="background-color:#fff">
	
	<div class="container">
		<div class="page-header">
          <h3 class="text-muted"><i class="fa fa-briefcase muted"></i>&nbsp;Experiencia</h3>
        </div>

		{% block experience_private %}
		{% use 'PCVDemoBundle:Show:experiencia.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_experience %}{{parent()}}{% endblock %}
	</div>



	<div class="container">
		<div class="page-header">
          <h3 class="text-muted"><i class="fa fa-book muted"></i>&nbsp;Educaci√≥n</h3>
        </div>

		{% block education_private %}
		{% include 'PCVDemoBundle:Show:educacion.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_education %}{{parent()}}{% endblock %}
	</div>

</div>

<div class="jumbotron" style="background-color:#f5f5f5">
	<div class="container">
		
		<div class="page-header" style="border-bottom-color:#ddd">
          	<h3 class="text-muted"><i class="fa fa-language muted"></i>&nbsp;Idiomas</h3>
        </div>
		{% embed 'PCVDemoBundle:Show:idiomas.html.twig' %}
			{% block idiom_private %}
				{{parent()}}
			{% endblock %}
			{% block modal_idiom %}{{parent()}}{% endblock %}
		{% endembed %}
		

		
		<div class="page-header" style="border-bottom-color:#ddd">
          	<h3 class="text-muted"><i class="fa fa-lightbulb-o muted"></i>&nbsp;Conocimientos</h3>
        </div>
		{% block knowing_private %}
			{% include 'PCVDemoBundle:Show:conocimientos.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_knowing %}{{parent()}}{% endblock %}



		<div class="page-header" style="border-bottom-color:#ddd">
          	<h3 class="text-muted"><i class="fa fa-star muted"></i>&nbsp;Competencias</h3>
        </div>
		{% block ability_private %}
			{% include 'PCVDemoBundle:Show:habilidades.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_ability %}{{parent()}}{% endblock %}
		

	</div>
</div>

<div class="jumbotron" style="background-color:#fff">

	<div class="container">
		<div class="page-header">
          <h3 class="text-muted"><i class="fa fa-trophy muted"></i>&nbsp;Reconocimientos y Premios</h3>
        </div>

		{% block premios_private %}
			{% include 'PCVDemoBundle:Show:premios.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_premios %}{{parent()}}{% endblock %}
	</div>
</div>

<div class="jumbotron" style="background-color:#f5f5f5">
	<div class="container">
		<div class="page-header">
          <h3 class="text-muted"><i class="fa fa-file-text muted"></i>&nbsp;Certificaciones</h3>
        </div>

		{% block certificaciones_private %}
		{% include 'PCVDemoBundle:Show:certificaciones.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_certificaciones %}{{parent()}}{% endblock %}
	</div>

</div>


<div class="jumbotron" style="background-color:#fff">
	<div class="container">
		<div class="page-header" style="border-bottom-color:#ddd">
          <h3 class="text-muted"><i class="fa fa-folder muted"></i>&nbsp;Portafolio</h3>
        </div>
        
		{% block project_private %}
		{% include 'PCVDemoBundle:Show:proyectos.html.twig' %}
			{{parent()}}
		{% endblock %}
		{% block modal_project %}{{parent()}}{% endblock %}
	</div>
</div>


{% endblock %}


	
{% block footer_page %}
    <!-- mostramos el texto de cabecera  -->
    {% use 'PCVDemoBundle:Contenido:contenido.html.twig' %}
    {% block footer %}
        {{parent()}}
    {% endblock %}
    <!-- end embed -->
{% endblock %}